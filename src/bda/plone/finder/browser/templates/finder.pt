<div class="finder_container">

  <div class="finder_message"></div>
  
  <div class="finder_dialog">
    <div class="text">
    </div>
    <div class="buttons">
      <button class="submit">OK</button> 
      <button class="cancel">Cancel</button> 
    </div>
  </div>

  <div class="finder_controls">
    <div class="buttonPrev">
      <a class="prev browse left"
         title="Back">&nbsp;</a>
    </div>
    <div class="buttonNext">
      <a class="next browse right"
         title="Next">&nbsp;</a>
    </div>
    
    <div class="buttonSeperator">&nbsp;</div>
    
    <div class="action_view">
      <a class="button disabled"
         title="View">&nbsp;</a>
    </div>
    <div class="action_edit">
      <a class="button disabled"
         title="Edit">&nbsp;</a>
    </div>
    <div class="action_change_state">
      <a class="button disabled"
         title="Change state">&nbsp;</a>
      <ul class="action_dropdown">
      </ul>
    </div>
    <div class="action_add_item">
      <a class="button disabled"
         title="Add new Item">&nbsp;</a>
      <ul class="action_dropdown">
      </ul>
    </div>
    
    <div class="buttonSeperator">&nbsp;</div>
    
    <div class="action_cut">
      <a class="button disabled"
         title="Cut">&nbsp;</a>
    </div>
    <div class="action_copy">
      <a class="button disabled"
         title="Copy">&nbsp;</a>
    </div>
    <div class="action_paste">
      <a class="button disabled"
         title="Paste">&nbsp;</a>
    </div>
    <div class="action_delete">
      <a class="button disabled"
         title="Delete">&nbsp;</a>
    </div>
    
    <input type="text"
           value="Filter focused column"
           class="column_filter" />
  </div>
  
  <div class="visualClear"></div>
  
  <div class="finder_columns">
    <div class="items">
      <tal:column repeat="column view/columns">
        <tal:column replace="structure column" />
      </tal:column>
    </div>
  </div>

</div>